    "@react-native-firebase/app": "^12.3.0",
    "@react-native-picker/picker": "^1.16.3",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@sentry/react-native": "^2.6.0",
    "@shopify/restyle": "^1.4.0",
    "@types/humps": "^2.0.1",
    "buffer": "^6.0.3",
    "event-emitter": "^0.3.5",
    "humps": "^2.0.1",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "mobx": "^6.1.0",
    "mobx-react-lite": "3.2.0",
    "mobx-state-tree": "^5.0.2",
    "moment": "^2.29.1",
    "mst-async-task": "^1.0.3",
    "query-string": "^7.0.1",
    "react": "17.0.1",
    "react-hook-form": "^7.12.0",
    "react-native": "0.64.1",
    "react-native-config": "^1.4.3",
    "react-native-device-info": "^8.1.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-localize": "^2.1.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-simple-crypto": "^0.2.15",
    "react-native-splash-screen": "^3.2.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "^8.1.0",
    "utility-types": "^3.10.0",
    "yup": "^0.32.9"
