    "npm-run-all": "^4.1.5",
    "react-cosmos": "^5.6.3",
    "react-dom": "^17.0.1",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": "yarn eslint --max-warnings=0"
  }
