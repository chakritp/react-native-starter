{
  "name": "TypescriptStarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "launch": "node scripts/launch",
    "android": "yarn launch android",
    "android:menu": "adb shell input keyevent 82",
    "android:reload": "adb shell input text 'RR'",
    "ios": "yarn launch ios",
    "ios:bundle": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output main.jsbundle",
    "ios:sim": "yarn launch ios --simulator 'iPhone 11'",
    "ios:install": "cd ios && pod install",
    "assets:link": "react-native link",
    "entry": "node scripts/generate-entry",
    "entry:switch": "node scripts/generate-entry --switch",
    "start": "react-native start --reset-cache",
    "cosmos": "cosmos-native",
    "dev": "yarn entry && npm-run-all --parallel start cosmos",
    "test": "jest",
    "lint": "eslint . --ext .ts --ext .tsx --ext .js --ext .jsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.6.1",
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/datetimepicker": "^3.5.2",
